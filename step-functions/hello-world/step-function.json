{
  "Comment": "A Hello World example of the Amazon States Language using Pass states",
  "StartAt": "Hello",
  "States": {
    "Hello": {
      "Type": "Task",
      "Resource": "Hello",
      "Next": "World"
    },
    "World": {
      "Type": "Task",
      "Resource": "World",
      "End": true
    }
  }
}
